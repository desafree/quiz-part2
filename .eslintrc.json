{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb","prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {"no-unused-vars":"warn",
        "prefer-const":"off",
        "no-use-before-define":"off"
    }
}
